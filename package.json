{
  "name": "VizualRegressionTesting",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "detox:ios": "__TESTING_PLATFORM__='ios' yarn detox  test --configuration ios",
    "detox:android:local": "__TESTING_PLATFORM__='android' yarn detox  test --configuration androidLocal",
    "detox:android:ci": "__TESTING_PLATFORM__='android' yarn detox  test --configuration androidCI",
    "detox:ios:build": " yarn detox  build --configuration ios",
    "detox:android:build:local": "yarn detox  build --configuration androidLocal",
    "detox:android:build:ci": "yarn detox  build --configuration androidCI",
    "detox:ios:update": "__TESTING_PLATFORM__='ios' DETOX_ARGV_OVERRIDE='--update-snapshot' yarn detox  test --configuration ios",
    "detox:android:update": "__TESTING_PLATFORM__='android' DETOX_ARGV_OVERRIDE='--update-snapshot' yarn detox  test --configuration androidLocal",
    "owl:android": "__TESTING_PLATFORM__='android' yarn owl test -p android",
    "owl:android:build": "yarn owl build -p android",
    "owl:android:update": "__TESTING_PLATFORM__='android' yarn owl test -p android -u",
    "owl:ios": "__TESTING_PLATFORM__='ios' yarn owl test -p ios",
    "owl:ios:build": "yarn owl build -p ios",
    "owl:ios:update": "__TESTING_PLATFORM__='ios' yarn owl test -p ios -u"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "lodash.kebabcase": "^4.1.1",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0"
  },
  "devDependencies": {
    "//": " for detox jest: 28.x.x / for own jest: 26.6.3",
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.0.2",
    "@types/react-native": "^0.69.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "detox": "^19.12.1",
    "eslint": "^7.32.0",
    "jest": "28.x.x",
    "jest-image-snapshot": "^5.2.0",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-native-owl": "1.1.0",
    "react-test-renderer": "18.0.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
